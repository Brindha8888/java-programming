import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
Scanner sc=new Scanner (System.in);
int bal=sc.nextInt();
int n=sc.nextInt();
switch(n){
    case 1:
int ip=sc.nextInt();
bal=ip+bal;
System.out.println(bal);
break;
    case 2:
int wd=sc.nextInt();

if(wd<bal){
bal=bal-wd;
System.out.println(bal);
}
else 
System.out.println("Insufficient Balance");
break;

        default:
System.out.println("Invalid Input");
}
    }
}
