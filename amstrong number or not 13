import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
int num=sc.nextInt();
int temp=num,ams=0;
if(num>99 && num<1000){
while(num>0){
int rem=num%10;
ams+=Math.pow(rem,3);
num=num/10;
}
if(temp==ams)
System.out.println("Yes");
else
System.out.println("No");
}
else
System.out.println("No");
   }
}
