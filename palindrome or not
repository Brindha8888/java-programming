import java.io.*;
import java.util.*;

public class Solution {

public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
int num=sc.nextInt();
if(num>=100 && num<1000){
int n= num/100;
int rem=num%10;
if(n==rem)
System.out.println("palindrome");
else
System.out.println("not palindrome");
}else{
System.out.println("Invalid Input");}
  }
}
