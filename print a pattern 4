import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
Scanner sc=new Scanner (System.in);
char c='A';
int n=sc.nextInt();
if(n<=9 && n>0){
for(int i=0;i<n;i++){ 
for(int j=0;j<=i;j++){
System.out.print(c++ +" " ) ;
}
System.out.println();
} 
}
else
System.out.println("Invalid Input");
 }
}
