import java.io.*;
import java.util.*;

public class Solution {

public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
int num=sc.nextInt();
int rem=0;
if(num>0 && num<=1000000000) {
while (num>0){
rem=num%10;
num=num/10;
System.out.print(rem);
}

} 
else
 System.out.println("Invalid Input");  }
}
