import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
int s=sc.nextInt();
int[] arr=new int[s];
for(int i=0;i<s;i++){
arr[i]=sc.nextInt();
} 
int max=arr[0];
int min=arr[0];
for(int i=0;i<s;i++){
if(arr[i]>max)
max=arr[i];
else if(arr[i]<min)
min=arr[i];
}
System.out.println("Youngest="+min);
System.out.println("Oldest="+max);
   
}
}
